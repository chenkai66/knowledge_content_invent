# Environment Variables for Knowledge Content Generation
# Copy this file to .env and add your actual API keys

# LLM API Keys
# Choose ONE of the following:

# DashScope (Alibaba Cloud)
VITE_DASHSCOPE_API_KEY=your-dashscope-api-key-here

# IdealLab (Internal Alibaba)
VITE_IDEALAB_API_KEY=your-idealab-api-key-here

# OpenAI
VITE_OPENAI_API_KEY=your-openai-api-key-here

# Base URLs for API endpoints
VITE_DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
VITE_IDEALAB_BASE_URL=https://idealab.alibaba-inc.com/api/openai/v1
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# Default model to use
VITE_DEFAULT_MODEL=qwen-max-latest

# Enable backend proxy by default to avoid CORS issues
# Set to 'true' to route all LLM requests through the backend proxy
VITE_USE_BACKEND_PROXY=true

# Enable detailed logging (development only)
VITE_DEBUG_LOGGING=false